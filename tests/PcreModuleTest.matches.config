DebugLogLevel 99
LoadModule "ibmod_htp.so"
LoadModule "ibmod_pcre.so"
LoadModule "ibmod_rules.so"
Set parser "htp"

# Request gets overwritten on purpose.
Rule request_headers "@pcre (.*)" id:pcre_request phase:REQUEST_HEADER "log:It worked."

# Overwrite the request headers. We assert on the response headers.
Rule response_headers "@pcre (.*), (.*)" id:pcre_response phase:RESPONSE_HEADER "log:It worked."
